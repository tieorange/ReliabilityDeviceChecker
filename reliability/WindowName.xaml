<Window x:Class="reliability.WindowName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Введіть ім'я" Height="109" Width="270" Closing="Window_Closing_1" Closed="Window_Closed_1">
    <Grid>
        <TextBox x:Name="TbName" HorizontalAlignment="Left" Height="23" Margin="48,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="196" MaxLength="50" MaxLines="1"/>
        <Label Content="Ім'я:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <Button x:Name="BtnOk" Content="OK" HorizontalAlignment="Left" Margin="72,40,0,0" VerticalAlignment="Top" Width="145" Click="BtnOk_Click" IsDefault="True" />
    </Grid>
</Window>
